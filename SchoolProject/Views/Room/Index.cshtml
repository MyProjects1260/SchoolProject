@model List<Room>

<a class="nav-link text-dark" asp-area="" asp-controller="Room" asp-action="Create">Add new room</a>
<br />
<table>
    <tr>
        <th>
            Room Name
        </th>
        <th>
            Delete
        </th>
    </tr>

    @foreach (Room room in Model)
    {
        <tr>
            <td>
                @room.RoomName
            </td>
            <td>
                <a href="/Teacher/Delete/@room.RoomId">Delete</a>
            </td>
        </tr>
    }

</table>




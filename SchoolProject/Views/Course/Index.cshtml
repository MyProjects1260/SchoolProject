@model List<Course>

<a class="nav-link text-dark" asp-area="" asp-controller="Course" asp-action="Create">Add new course</a>
<br />
<table>
    <tr>
        <th>
            Course Name
        </th>
        <th>
            Course Capacity
        </th>
        <th>
            Delete
        </th>
    </tr>

    @foreach (Course course in Model)
    {
        <tr>
            <td>
                @course.CourseName
            </td>
            <td>
                @course.CourseCapacity
            </td>
            <td>
                <a href="/Teacher/Delete/@course.CourseId">Delete</a>
            </td>
        </tr>
    }

</table>



